<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
	"http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="log" extends="struts-default" namespace="/log">
		<!-- 日志 -->
		<action name="Log" class="com.hx.framework.log.LogAction">
            <result>/jsp/framework/log/Log.jsp</result>
            <result name="detail">/jsp/framework/log/LogDetail.jsp</result>
        </action>
        
        <!-- 系统日志 -->
        <action name="LogSys" class="com.hx.framework.log.LogSystemAction">
            <result>/jsp/framework/log/LogSystem.jsp</result>
            <result name="detail">/jsp/framework/log/LogSystemDetail.jsp</result>
        </action>
	</package>
</struts>
